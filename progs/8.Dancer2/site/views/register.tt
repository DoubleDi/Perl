<!DOCTYPE html>
<html>
<head>
	<title>last_five</title>
    <link rel="stylesheet" href="/css/bootstrap.min.css" />
    <link rel="stylesheet" href="/css/style.css" />
    <link rel="stylesheet" href="/css/login.css" />
</head>
<body>
    <div class="container" style="margin-top:30px">
    <div class="col-md-4 col-md-offset-4">
        <div class="panel panel-default">
      <div class="panel-heading text-center"><h3 class="panel-title"><strong>Register </strong></h3>
      </div>

      <div class="panel-body">
       <form role="form" action="/reg" method="post">
    				<div class="form-group">
                        <input type="text" name="nickname" id="nickname" class="form-control" placeholder="Nickname">
        				<input type="text" name="avatar" id="avatar" class="form-control " placeholder="Avatar(link)">
        				<input type="text" name="password" id="password" class="form-control " placeholder="Password">
        				<input type="text" name="confirm_password" id="confirm_password" class="form-control " placeholder="Confirm password">
        			</div>
                <button type="submit" class="btn-block btn btn-success">Sign Up</button>
    </div>
    </form>
    </div>
    <a href="/login" class="btn-block text-center btn-lg btn-success">Login</a>
    </div>
    </div>
    </div>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js"></script>
    <script src="javascripts/bootstrap.min.js"></script>
    <script src="javascripts/login.js"></script>

  </body>
</html>

</body>
</html>
